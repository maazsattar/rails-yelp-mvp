<div class="container">
  <h1>New Restaurant</h1>
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category, collection: Restaurant::COLLECTION %>
    <%= f.submit %>
  <% end %>
  <h5><%= link_to "Back", restaurants_path, class: "btn btn-secondary" %></h5>
</div>
